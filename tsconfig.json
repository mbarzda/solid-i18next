{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "resolveJsonModule": true,
    "target": "esnext",
    "paths": {
      "@mbarzda/solid-i18next": ["./src/index.ts"],
      "$/*": ["./docs/*"]
    }
  }
}
