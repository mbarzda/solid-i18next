{
  "author": "Martynas Barzda <martynas@barzda.pro>",
  "browser": "./dist/index.mjs",
  "description": "i18next for SolidJS",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/jest": "^29.1.2",
    "babel-preset-solid": "^1.5.7",
    "html-parse-string": "^0.0.8",
    "i18next": "^21.10.0",
    "i18next-http-backend": "^1.4.4",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "rimraf": "^3.0.2",
    "solid-jest": "^0.2.0",
    "solid-js": "^1.5.7",
    "solid-testing-library": "^0.3.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.7",
    "vite-plugin-solid": "^2.3.9"
  },
  "exports": {
    ".": {
      "browser": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "solid": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "i18n",
    "i18next",
    "internationalization",
    "intl",
    "language",
    "localization",
    "solid",
    "solid-js",
    "solidjs",
    "translate",
    "translations"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "name": "@mbarzda/solid-i18next",
  "peerDependencies": {
    "i18next": ">=21.x",
    "solid-js": ">=1.5.x"
  },
  "repository": "github:mbarzda/solid-i18next",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "vite build --config vite.build.config.js",
    "prebuild:dev": "rimraf ./docs",
    "build:docs": "vite build --config vite.docs.config.js",
    "postbuild:docs": "cp -r ./docs/locales ./dist/locales && cp -r ./docs/favicon.ico ./dist/favicon.ico",
    "docs": "vite --config vite.docs.config.js",
    "pretest": "rimraf ./coverage",
    "test": "jest --coverage",
    "test:dev": "jest"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "version": "1.2.0"
}
